{
  "compilerOptions": {
    "target": "es5",
    "allowJs": true,
    "alwaysStrict": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "rootDir": ".",
    "lib": ["dom", "es2017"],
    "module": "esnext",
    "baseUrl": ".",
    "moduleResolution": "node",
    "noEmit": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,


    "jsx": "preserve",
    "incremental": true
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "gen/index.ts"],
  "exclude": ["node_modules"]
}
